<?xml version= '1.0' encoding= 'UTF-8' ?>
<schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="my.org/pr"
     xmlns:pr="my.org/pr" xmlns="http://www.w3.org/2001/XMLSchema">
 
    <element name="SpecialEvent" type="pr:specialEventType"/>
